<HTML>
<HEAD>
<TITLE>Add ZClass</TITLE>
</HEAD>
<BODY BGCOLOR="#FFFFFF" LINK="#000099" VLINK="#555555">
<H2>Add ZClass</H2>

<!--#with "_(selected=[], unselected=[], baseclasses=[], id='', title='',
             meta_type='',
             all=manage_subclassableClassNames(),
             )"-->
<!--#with REQUEST-->
<!--#with "_(
    selected=_.reorder(
       all,
       with   = baseclasses+
                (REQUEST.get('manage_addZClassForm','')=='    -> '
                 and unselected or []),
       without= (REQUEST.get('manage_addZClassForm','')==' <-    ' 
                 and   selected or [])
       ),
    )"-->
<!--#with "_(unselected=_.reorder(all, without=selected))"-->

<FORM ACTION="." METHOD="GET"><TABLE>

<TR>
  <TH ALIGN="LEFT" VALIGN="TOP">Id</TH>
  <TD ALIGN="LEFT" VALIGN="TOP" COLSPAN="3">
    <INPUT TYPE="TEXT" NAME="id" SIZE="40" VALUE="<!--#var id-->">
  </TD>
</TR>

<TR>
  <TH ALIGN="LEFT" VALIGN="TOP"><EM>Title</EM></TH>
  <TD ALIGN="LEFT" VALIGN="TOP" COLSPAN="3">
    <INPUT TYPE="TEXT" NAME="title" SIZE="40" VALUE="<!--#var title-->">
  </TD>
</TR>

<TR>
  <TH ALIGN="LEFT" VALIGN="TOP">
    Meta type</TH>
  <TD ALIGN="LEFT" VALIGN="TOP" COLSPAN="3">
    <INPUT TYPE="TEXT" NAME="meta_type" SIZE="40" 
       VALUE="<!--#var meta_type-->">
  </TD>
</TR>

<TR>
  <TD ALIGN="LEFT" VALIGN="TOP" COLSPAN="2">
    <INPUT TYPE="CHECKBOX" NAME="CreateAFactory:int" VALUE="1"
    <!--#if CreateAFactory-->CHECKED<!--#/if-->>
    <STRONG>Create constructor objects.</STRONG>
  </TD>
</TR> 

<tr>
<TR>
  <TH ALIGN="LEFT" VALIGN="TOP">Base classes</TH>
  <TD ALIGN="LEFT" VALIGN="TOP">
     Unselected:<BR>
     <SELECT NAME="unselected:list" size=9 multiple>
       <!--#in unselected-->
         <OPTION VALUE="<!--#var sequence-key-->"
	 ><!--#var sequence-item--></OPTION>
       <!--#/in-->
     </SELECT>
  </TD>
  <TD ALIGN="LEFT" VALIGN="MIDDLE">
     <!--#in selected-->
        <input type=hidden name="baseclasses:list"
	       value="<!--#var sequence-key-->"><br>
     <!--#/in-->
     <INPUT TYPE="SUBMIT" NAME="manage_addZClassForm:method" 
            VALUE="    -> ">
     <BR>
     <INPUT TYPE="SUBMIT" NAME="manage_addZClassForm:method" 
            VALUE=" <-    ">
  </TD>
  <TD ALIGN="LEFT" VALIGN="TOP">
     Selected:<br>
     <SELECT NAME="selected:list" size=9 multiple>
       <!--#in selected-->
         <OPTION VALUE="<!--#var sequence-key-->"
         ><!--#var sequence-item--></OPTION>
       <!--#/in-->
     </SELECT>
  </TD>
</TR>

<TR><TD></TD>
<TD colspan=3><BR><INPUT TYPE="SUBMIT" NAME="manage_addZClass:method" 
         VALUE=" Add "></TD>
</TR>
</TABLE></FORM>

<!--#/with-->
<!--#/with-->
<!--#/with-->
<!--#/with-->

</BODY></HTML>
















