<TR>
  <TD ALIGN="LEFT" VALIGN="TOP">
  <STRONG>Process Id</STRONG>
  </TD>
  <TD ALIGN="LEFT" VALIGN="TOP">
  <!--#var process_id-->
  </TD>
</TR>
<TR>
  <TD ALIGN="LEFT" VALIGN="TOP">
  <STRONG>Process Uptime</STRONG>
  </TD>
  <TD ALIGN="LEFT" VALIGN="TOP">
  <!--#var process_time-->
  </TD>
</TR>
<TR>
  <TD ALIGN="LEFT" VALIGN="TOP">
  <STRONG>Database Size</STRONG>
  </TD>
  <TD ALIGN="LEFT" VALIGN="TOP">
  <!--#var db_size-->
  </TD>
</TR>
<TR>
  <TD ALIGN="LEFT" VALIGN="TOP">
  <STRONG>Database Location</STRONG>
  </TD>
  <TD ALIGN="LEFT" VALIGN="TOP">
