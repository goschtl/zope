<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN" "http://www.w3.org/TR/REC-html40/loose.dtd">
<html>
<head>
<meta http-equiv="content-type" content="text/html; charset=iso-8859-1">
<title><dtml-if title><dtml-var title></dtml-if></title>

<dtml-let ag="REQUEST.get('HTTP_USER_AGENT', '')"
     is_nav4="ag[:9] == 'Mozilla/4' and _.string.find(ag, 'MSIE') < 0"
     use_css="REQUEST.get('zmi_use_css', is_nav4 and 'inline' or '1')">
<dtml-if use_css>
  <dtml-if expr="use_css=='inline'">
    <style type="text/css"><!--

      <dtml-var manage_page_style.css>

    --></style>
  <dtml-else>
    <link rel="stylesheet" type="text/css"
     href="<dtml-var BASEPATH1>/manage_page_style.css">
  </dtml-if>
</dtml-if>
</dtml-let>

</head>
<body bgcolor="#ffffff" link="#000099" alink="#000099" vlink="#000099">
