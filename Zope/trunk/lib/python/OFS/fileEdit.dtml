<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN" "http://www.w3.org/TR/REC-html40/loose.dtd">
<HTML lang="en">
<HEAD>
<TITLE>Edit</TITLE>
</HEAD>
<BODY BGCOLOR="#FFFFFF" LINK="#000099" VLINK="#555555">
<dtml-var manage_tabs>

<FORM ACTION="manage_edit" METHOD="POST">
  <TABLE CELLSPACING="2">
    <TR>
      <TH ALIGN="LEFT" VALIGN="TOP">Id</STRONG></TH>
      <TD ALIGN="LEFT" VALIGN="TOP"><dtml-var id></TD>
    </TR>
    <TR>
      <TH ALIGN="LEFT" VALIGN="TOP"><EM>Title</EM></TH>
      <TD ALIGN="LEFT" VALIGN="TOP">
        <INPUT TYPE="TEXT" NAME="title" SIZE="40" VALUE="<dtml-var title html_quote>">
      </TD>
    </TR>
    <TR>
      <TH ALIGN="LEFT" VALIGN="TOP">Content Type</TH>
      <TD ALIGN="LEFT" VALIGN="TOP">
        <INPUT TYPE="TEXT" NAME="content_type:required" SIZE="40"
           VALUE="<dtml-var content_type html_quote>">
      </TD>
    </TR>
    <TR>
      <TH ALIGN="LEFT" VALIGN="TOP"><em>Precondition</em></TH>
      <TD ALIGN="LEFT" VALIGN="TOP">
        <INPUT TYPE="TEXT" NAME="precondition" SIZE="40"
           VALUE="<dtml-var precondition html_quote>">
      </TD>
    </TR>
    <TR>
      <TH ALIGN="LEFT" VALIGN="TOP">Size</TH>
      <TD ALIGN="LEFT" VALIGN="TOP"><dtml-var size thousands_commas> bytes</TD>
    </TR>
    <TR>
      <TH ALIGN="LEFT" VALIGN="TOP">Last modified</TH>
      <TD ALIGN="LEFT" VALIGN="TOP"><dtml-var bobobase_modification_time></TD>
    </TR>
    <TR>
      <TD></TD><TD><BR><INPUT TYPE="SUBMIT" VALUE="Change"></TD>
    </TR>
  </TABLE>
</FORM>
</BODY>
</HTML>

