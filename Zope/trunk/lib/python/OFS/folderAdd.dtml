  <TD ALIGN="LEFT" VALIGN="TOP"><INPUT TYPE="TEXT" NAME="id" SIZE="50"></TD>
</TR>
<TR>
  <TD ALIGN="LEFT" VALIGN="TOP"><B>Title</B></TD>
  <TD ALIGN="LEFT" VALIGN="TOP"><INPUT TYPE="TEXT" NAME="title" SIZE="50"></TD>
</TR>
<TR>
  <TD></TD>
  <TD>
  <TABLE>
  <TR>
  <TD VALIGN="TOP">
    <INPUT TYPE="RADIO" NAME="acl_type" VALUE="E">
