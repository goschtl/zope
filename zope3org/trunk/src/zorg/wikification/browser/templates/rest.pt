<div id="main">
    <div class="col" id="col">
    <div id="edit_box">
        <div xmlns:metal="http://xml.zope.org/namespaces/metal">
            <form action="" method="POST" tal:attributes="action string:${context/@@absolute_url}/@@wikiupdate.html">
                <input type="hidden" id="asContentType" value="text/plain" />
                <input type="hidden" id="editor" value="rest" />
                
                <h4>Title</h4>
    			<input class="title" id="version_title" name="version[title]" size="30" type="text" value="Test" 
    			             tal:attributes="value string:${view/editableTitle}"/>
                <h4>Body</h4>

                <textarea class="edit-area" tal:content="view/display" 
                        id="version_body" name="rest" cols="68" rows="22" value=""></textarea>
                <br/>
                <p><input name="text_submit" type="submit" value="Save">
                or <a href="./wiki.html">Cancel</a></p>
                <p>Switch to <a href="./wikiedit.html?editor=kupu">Kupu</a>
                or <a href="./wikiedit.html?editor=tinymce">Tiny MCE</a>
            </form>
        </div>
    </div>
    </div>

</div>

	