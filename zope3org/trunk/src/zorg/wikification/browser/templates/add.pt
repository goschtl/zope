<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      >

  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <title>Add Page</title>
        
    <script type="text/javascript" src="prototype.js" tal:attributes="src string:${context/++resource++zorgajax/prototype.js}"></script>
    <script type="text/javascript" src="managers.js" tal:attributes="src string:${context/++resource++wikiscripts/managers.js}"></script>

    <link rel="stylesheet" type="text/css" media="print" href="/@@/style/print.css" />
    <link rel="stylesheet" type="text/css" media="screen" href="/@@/style/screen.css" />
    <link rel="stylesheet" type="text/css" media="screen" href="/@@/style/main.css" />
   
  </head>

  <body>
  <div id="layout">
    <div id="layout-header" class="columns" tal:define="uris view/uris">
		<div class="col first">
			<div id="banner">
				<a tal:attributes="href uris/home">
					<img src="/@@/images/circle-z.png" alt="(circle-z)" title="" height="36" width="36" />
					Zope 3 
				</a>
			</div>
		</div>	
		<div class="col second">
           <div id="action-navigation"  class="itemViews">
			  <a href="@@login.html">Login</a> &nbsp; |  &nbsp;
              <a href="@@wiki.html">View</a> &nbsp; |  &nbsp;
              Edit &nbsp;&nbsp;&nbsp;&nbsp;
           </div>
		</div>		
    </div>
    <div id="navigation-box">
        <span id="breadcrumb-navigation" metal:use-macro="context/@@wiki_macros/breadcrumbs" />
    </div>
    
    <div id="layout-columns" class="columns">
        <div class="bg">
            <div class="col first">
                <div id="main" tal:replace="structure view/main/render">
                </div>
            </div>
            <div class="col second">
                <div metal:use-macro="context/@@wiki_macros/navigation_tree_box" />
            </div>
        </div>
    </div>

</body>
</html>
