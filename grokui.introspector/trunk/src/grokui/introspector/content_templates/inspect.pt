<html>
        <h1>Content browser</h1>

    <h2>Content</h2>
    <tal:content define="content context/keys | nohting" >
        <span tal:condition="not: content">
            No content found...
        </span>
        <span tal:condition="content"
              tal:repeat="item content">
               <div tal:content="item" /> 
        </span>
    </tal:content>
    <h2>Object Info</h2>
    <span tal:content="context"/>

</html>